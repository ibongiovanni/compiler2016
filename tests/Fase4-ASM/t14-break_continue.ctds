class main{
	void print_int(integer n) extern;
	void print_bool(bool b) extern;	
	void print_line() extern;


	integer main(){
		integer i,sum,max;
		max=10;
		i=0;
		while(i<max){
			i=i+1;
			sum=sum+i;
			if (i%2==0)
			{
				print_int(i);
				continue;
			}
			if (sum>40)
			{
				break;
			}
		}
		print_line();
		print_int(sum);
		return 0;
	}
}